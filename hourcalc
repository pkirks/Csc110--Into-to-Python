def find_unique(yobs) -> list:
    '''
    > read_file('hours.csv')
    return something
    '''
    lizt= []
    for i in yobs:
        if i not in lizt:
            lizt.append(i)

    return lizt

#driver
yobs = [22159, 22328, 22359, 22159, 22359, 23042, 23102, 22359, 22162, 23000, 22252]
unique = find_unique(yobs)
print('unique jobs:', unique)



